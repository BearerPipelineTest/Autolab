<!-- Latest Sortable -->
<script src="https://raw.githack.com/SortableJS/Sortable/master/Sortable.js"></script>
<table id="entries">
  <tr id="tableHeaders" class="list-group">
    <th class="i">#</th>
    <th class="email list-group-item">Email</th>
  <% problems.each do |problem| %>
    <th class="list-group-item"><%= problem.name %></th>
  <% end %>
    <th class="list-group-item">Grade Type</th>
  </tr>
<% entries.each_with_index do |entry, i| %>
  <tr>
    <td class="i"><%= i %></td>
    <td class="email <%= bulkGrade_cell_class entry[:email] %>"><%= bulkGrade_cell entry[:email] %>
  <% problems.each do |problem| %>
    <td class="datum <%= bulkGrade_cell_class entry[:data][problem.name] %>"><%= bulkGrade_cell entry[:data][problem.name] %></td>
  <% end %>
    <td class="grade_type <%= bulkGrade_cell_class entry[:grade_type] %>"><%= bulkGrade_cell entry[:grade_type] %></td>
  </tr>
<% end %>
</table>
<script>Sortable.create(tableHeaders, { /* options */ });</script>